<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Internship Records</title>
</head>
<body>
  <h1>Internship Records</h1>
  <table border="1">
    <tr>
      <th>Internship ID</th>
      <th>Intern ID</th>
      <th>Start Date</th>
      <th>End Date</th>
      <!-- Add more columns as needed -->
    </tr>
    <% data.forEach((row) => { %>
      <tr>
        <td><%= row.internship_id %></td>
        <td><%= row.intern_id %></td>
        <td><%= row.start_date %></td>
        <td><%= row.end_date %></td>
        <!-- Add more columns as needed -->
      </tr>
    <% }); %>
  </table>
</body>
</html>
